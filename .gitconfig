[core]
        excludesfile = "~/.gitignore_global"
[user]
	name = Hatanaka Taiga
	email = hatanaka-hiroyoshi@kayac.com
[color]
	ui = auto
[alias]
        st = status
        co = checkout
        ci = commit
        br = branch
        graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
[push]
	default = simple
